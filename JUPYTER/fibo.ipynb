   "source": [
    "cache = {0: 0, 1: 1}\n",
    "def fibo_recur(n):\n",
    "    if not n in cache:\n",
    "        cache[n] = fibo_recur(n - 1) + fibo_recur(n - 2)\n",
    "    return cache[n]\n"
   ]
  },
